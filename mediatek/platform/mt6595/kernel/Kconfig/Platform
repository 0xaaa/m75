config ARCH_MT6595
        bool "MediaTek MT6595"
        select GENERIC_TIME
        select GENERIC_CLOCKEVENTS
        select ARCH_HAS_CPUFREQ
        select ARM_AMBA
        select CPU_V7
        select HAVE_SMP
        select NEED_MACH_MEMORY_H
        select FIQ_GLUE
        select IRQ_DOMAIN
        select IRQ_DOMAIN_DEBUG
        help
          This enable support for MediaTek MT6595.
